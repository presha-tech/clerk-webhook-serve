{
  "expo": {
    "name": "BusinessDirectory",
    "slug": "BusinessDirectory",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "businessdirectory",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "entryPoint": "expo-router/entry",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.systechsoftwares.businessdirectory",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.systechsoftwares.businessdirectory",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a0d80d8a-026b-4f26-886a-eb427aca402f"
      }
    }
  }
}
